{
  "name": "preveltekit",
  "version": "1.0.0",
  "description" :"PrevelteKit is a lightweight, high-performance web application framework built on Svelte 5, featuring Server-Side Pre Rendering (SSPR) using Rsbuild and jsdom",
  "repository": "https://github.com/tbocek/preveltekit/",
  "author": "Thomas Bocek",
  "license": "MIT",
  "type": "module",
  "svelte": "dist/Router.svelte",
  "main": "dist/client.js",
  "types": "dist/client.d.ts",
  "exports": {
    ".": {
      "types": "./dist/client.d.ts", 
      "import": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./Router.svelte": "./dist/Router.svelte",
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.d.ts"
    }
  },
  "bin": {
    "preveltekit": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/** && tsc && cp src/Router.svelte src/types.d.ts dist/",
    "prepublishOnly": "npm run build",
    "docs": "cd example && pnpm install && pnpm run build && rm -f ../docs/* && mv dist/* ../docs"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.5.0",
    "@rsbuild/plugin-svelte": "^1.0.11",
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "express": "^5.1.0",
    "jsdom": "^27.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.0",
    "typescript": "^5.0.0"
  }
}